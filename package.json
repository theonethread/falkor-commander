{
  "name": "@falkor/falkor-commander",
  "version": "1.0.0-beta.0",
  "description": "Falkor operations commander",
  "author": {
    "name": "Barnabas Bucsy",
    "url": "https://github.com/theonethread"
  },
  "contributors": [
    {
      "name": "Barnabas Bucsy",
      "url": "https://github.com/theonethread"
    }
  ],
  "license": "MIT",
  "repository": "github:theonethread/falkor-commander",
  "homepage": "https://falkor.world/",
  "funding": {
    "type": "individual",
    "url": "https://www.linkedin.com/in/barnabas-bucsy"
  },
  "keywords": [
    "falkor",
    "operations",
    "commander"
  ],
  "type": "module",
  "main": ".dist/index.js",
  "typings": ".dist/index.d.ts",
  "bin": {
    "falkor-commander": ".dist/index.js"
  },
  "shared": ".dist/cli/version.js",
  "files": [
    ".dist",
    ".man"
  ],
  "man": [
    ".man/falkor-commander.1"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "debug": "rimraf .dist/**/* && falkor-bundler --debug --input src/index.ts && falkor-bundler --debug --input src/cli/version.ts --out .dist/cli",
    "release": "rimraf .dist/**/* && falkor-bundler --release --input src/index.ts && falkor-bundler --release --input src/cli/version.ts --out .dist/cli",
    "man": "pandoc --version && (mkdir .man || rimraf .man/**/*) && pandoc --standalone --to man ./man/man.md --output ./.man/falkor-commander.1",
    "prepublishOnly": "npm run release && npm run man"
  },
  "devDependencies": {
    "@types/node": "14.17.29",
    "@types/shelljs": "0.8.9",
    "@types/figlet": "1.5.4",
    "@types/semver": "7.3.9",
    "@types/minimist": "1.2.2",
    "rimraf": "3.0.2",
    "@falkor/falkor-bundler": "1.1.4"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "shelljs": "0.8.4",
    "@falkor/falkor-library": "1.0.0-beta.3"
  }
}
